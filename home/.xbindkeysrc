"playerctl play-pause"
    XF86AudioPlay

"playerctl next"
    XF86AudioNext

"playerctl previous"
    XF86AudioPrev

"pamixer -i 5 && kill -35 $(pidof dwmblocks)"
    XF86AudioRaiseVolume

"pamixer -d 5 && kill -35 $(pidof dwmblocks)"
    XF86AudioLowerVolume

"pamixer -t && kill -35 $(pidof dwmblocks)"
    XF86AudioMute
